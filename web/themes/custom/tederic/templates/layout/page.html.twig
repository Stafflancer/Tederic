{{ attach_library('tederic/load-page') }}

<div class="load-page">
  <img src="/themes/custom/tederic/logo.svg" alt="Tederic">
</div>

<div class="header-fixed">
  {{ page.header_top }}
  {{ page.header }}
</div>
<div class="content-wrapper">
  {{ page.highlighted }}
  {{ page.breadcrumb }}
  {{ page.content }}
  {{ page.footer_first }}
  {{ page.footer_second }}
</div>

<div class="button-up"></div>

<div id="contact-modal" title="{{ 'Get in touch'|t }}" class="hidden">
  {{ drupal_entity('webform', 'contact') }}
</div>
