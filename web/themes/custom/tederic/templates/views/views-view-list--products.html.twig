<div class="product-items">
  {% if attributes -%}
    <div{{ attributes }}>
  {% endif %}
    {% if title %}
      {{ title }}
    {% endif %}

    <{{ list.type }}{{ list.attributes }}>

      {% for row in rows %}
        <li{{ row.attributes }}>
          {{- row.content -}}
        </li>
      {% endfor %}

    </{{ list.type }}>

  {% if attributes -%}
    </div>
  {% endif %}
</div>
